{
  "install": {
    "trait": {
      "App\\Entity\\Product\\ProductVariant": {
        "add": {
          "LoevgaardSyliusBarcodePluginProductVariantTrait": "Loevgaard\\SyliusBarcodePlugin\\Model\\ProductVariantTrait"
        }
      }
    },
    "interface": {
      "App\\Entity\\Product\\ProductVariantInterface": {
        "add": {
          "barcodeProductVariantInterface": "Loevgaard\\SyliusBarcodePlugin\\Model\\ProductVariantInterface"
        }
      }
    },
    "config": {
      "config/packages/_sylius.yaml" : {
        "add": {
          "configFile": "@DHArtisBarcodePlugin/Resources/config/config.yml"
        }
      }
    }
  }
}
